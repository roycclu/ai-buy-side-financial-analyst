# Copy this file to .env and fill in your values

# Anthropic API Key (required when LLM_PROVIDER=anthropic)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Override the SEC EDGAR user agent
# SEC_EDGAR_USER_AGENT=YourName your@email.com

# LLM Provider â€” "anthropic" (default) or "ollama"
LLM_PROVIDER=ollama

# Local model to use when LLM_PROVIDER=ollama (see models/README.md)
LOCAL_MODEL=llama3.1:8b

# Ollama server URL (default shown)
OLLAMA_BASE_URL=http://localhost:11434
